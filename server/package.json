{
  "name": "fool-stuck-server",
  "version": "1.0.0",
  "description": "Express template generated by Fool-Stuck",
  "main": "index.js",
  "scripts": {
    "uninstall": "rimraf node_modules",
    "start": "nodemon src/app.js --exec babel-node --presets @babel/preset-env",
    "build": "babel-node --presets @babel/preset-env src -d dist --source-maps inline --copy-files",
    "serve": "node dist/app.js"
  },
  "author": "tiruri",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.6.5",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3"
  }
}
